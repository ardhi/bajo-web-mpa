{
  "alias": "mpa",
  "prefix": "",
  "mountAppAsRoot": true,
  "auth": ["session"],
  "charset": "utf-8",
  "viewEngine": "",
  "formatTagResult": true,
  "nunjucks": {
    "noCache": true
  },
  "prettier": {
    "enabled": false,
    "options": {
      "parser": "html",
      "printWidth": 200
    }
  },
  "minify": {
    "enabled": false,
    "options": {
      "removeAttributeQuotes": true,
      "removeComments": true,
      "removeCommentsFromCDATA": true,
      "removeCDATASectionsFromCDATA": true,
      "collapseWhitespace": true,
      "conservativeCollapse": true,
      "decodeEntities": true,
      "collapseBooleanAttributes": true,
      "removeRedundantAttributes": true,
      "removeEmptyAttributes": true
    }
  },
  "dependencies": ["bajo-web"]
}