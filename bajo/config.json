{
  "alias": "mpa",
  "title": "Multi Page Webapp",
  "prefix": "",
  "mountAppAsRoot": true,
  "charset": "utf-8",
  "defViewEngine": "lodash",
  "theme": "bootstrap5",
  "darkMode": {
    "preset": "light",
    "qsKey": "dark"
  },
  "i18n": {
    "detectors": ["qs"],
    "defaultNs": ["bajoDb", "bajoWebMpa"]
  },
  "emoji": true,
  "componentName": true,
  "prettier": {
    "parser": "html",
    "printWidth": 1000
  },
  "minify": {
    "removeAttributeQuotes": true,
    "removeComments": true,
    "removeCommentsFromCDATA": true,
    "removeCDATASectionsFromCDATA": true,
    "collapseWhitespace": true,
    "conservativeCollapse": true,
    "decodeEntities": true,
    "collapseBooleanAttributes": true,
    "removeRedundantAttributes": true,
    "removeEmptyAttributes": true
  },
  "markdown": {
    "tableClass": "table my-3",
    "tableHeadClass": "",
    "tableBodyClass": "table-group-divider"
  },
  "home": {
    "action": "redirect",
    "route": true
  },
  "multipart": {
  },
  "cors": {},
  "helmet": {
    "contentSecurityPolicy": false
  },
  "compress": false,
  "rateLimit": false,
  "traceNoTemplate": false,
  "disabled": [],
  "dependencies": ["bajo-web", "bajo-web-static"]
}