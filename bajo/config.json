{
  "alias": "mpa",
  "prefix": "",
  "mountAppAsRoot": true,
  "auth": ["session"],
  "charset": "utf-8",
  "viewEngine": "",
  "theme": "bootstrap5",
  "darkMode": {
    "preset": "light",
    "qsKey": "dark"
  },
  "formatTagResult": true,
  "nunjucks": {
    "noCache": true
  },
  "i18nDetectors": ["qs"],
  "prettier": {
    "enabled": false,
    "options": {
      "parser": "html",
      "printWidth": 1000
    }
  },
  "minify": {
    "enabled": false,
    "options": {
      "removeAttributeQuotes": true,
      "removeComments": true,
      "removeCommentsFromCDATA": true,
      "removeCDATASectionsFromCDATA": true,
      "collapseWhitespace": true,
      "conservativeCollapse": true,
      "decodeEntities": true,
      "collapseBooleanAttributes": true,
      "removeRedundantAttributes": true,
      "removeEmptyAttributes": true
    }
  },
  "siteInfo": {
    "title": "My Website",
    "orgName": "My Organization"
  },
  "home": {
    "action": "redirect",
    "route": true
  },
  "dependencies": ["bajo-web", "bajo-web-static"]
}